<div class="row">
<div class="large-6 columns">

<h1>Users</h1>
<table id="users">
<thead>
  <tr>
    <th>First name</th>
    <th>Last name</th>
  </tr>
</thead>
<tbody>
<% User.all.each do |user| %>
  <tr>
    <td><%= user.fname %></td>
    <td><%= user.lname %></td>
  </tr>
<% end %>
</tbody>
</table>
</div>
</div>